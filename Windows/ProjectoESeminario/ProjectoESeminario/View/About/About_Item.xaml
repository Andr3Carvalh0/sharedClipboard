<UserControl x:Class="ProjectoESeminario.View.About.About_Item"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectoESeminario.View.About"
             mc:Ignorable="d" 
             Background="#FFFFFF" d:DesignWidth="374" Height="Auto">
    <Grid Margin="1,0,0,0" Height="Auto" Background="#CECECE">
        <Grid Margin="0,0,0,1" Height="Auto" Background="#FFFFFF">
            <Label Name="Title" Content="• Text"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="363" FontSize="14" FontWeight="Bold" />
            <TextBlock TextWrapping="WrapWithOverflow" x:Name="Description" HorizontalAlignment="Left" Margin="10,23,0,0" VerticalAlignment="Top" Height="Auto" Width="Auto" FontWeight="Bold" Foreground="#FFBEBEBE" FontSize="11" >
                Text
            </TextBlock>
        </Grid>
    </Grid>
</UserControl>
